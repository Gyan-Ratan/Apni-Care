<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homepage">

    <fragment
        android:id="@+id/homepage"
        android:name="com.example.apnicare.homepage"
        android:label="fragment_homepage"
        tools:layout="@layout/fragment_homepage" >
        <action
            android:id="@+id/action_homepage_to_upload_prescription"
            app:destination="@id/upload_prescription" />
    </fragment>
    <fragment
        android:id="@+id/upload_prescription"
        android:name="com.example.apnicare.upload_prescription"
        android:label="fragment_upload_prescription"
        tools:layout="@layout/fragment_upload_prescription" >
        <action
            android:id="@+id/action_upload_prescription_to_why_prescription"
            app:destination="@id/why_prescription" />
        <action
            android:id="@+id/action_upload_prescription_to_why_prescription_2"
            app:destination="@id/why_prescription_2" />
        <action
            android:id="@+id/action_upload_prescription_to_upload_options"
            app:destination="@id/upload_options" />
    </fragment>
    <dialog
        android:id="@+id/why_prescription"
        android:name="com.example.apnicare.Upload_Prescription.why_prescription"
        android:label="fragment_why_prescription"
        tools:layout="@layout/fragment_why_prescription" >
        <action
            android:id="@+id/action_why_prescription_to_upload_prescription"
            app:destination="@id/upload_prescription" />
    </dialog>
    <dialog
        android:id="@+id/why_prescription_2"
        android:name="com.example.apnicare.Upload_Prescription.why_prescription_2"
        android:label="fragment_why_prescription_2"
        tools:layout="@layout/fragment_why_prescription_2" >
        <action
            android:id="@+id/action_why_prescription_2_to_upload_prescription"
            app:destination="@id/upload_prescription" />
    </dialog>
    <dialog
        android:id="@+id/upload_options"
        android:name="com.example.apnicare.Upload_Prescription.upload_options_prescription"
        android:label="fragment_search_medicine"
        tools:layout="@layout/fragment_upload_options_prescription" />
</navigation>