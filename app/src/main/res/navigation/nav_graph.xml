<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homepage">

    <fragment
        android:id="@+id/homepage"
        android:name="com.example.apnicare.homepage"
        android:label="fragment_homepage"
        tools:layout="@layout/fragment_homepage" >
        <action
            android:id="@+id/action_homepage_to_upload_prescription"
            app:destination="@id/upload_prescription" />
        <action
            android:id="@+id/action_homepage_to_my_cart"
            app:destination="@id/my_cart" />
    </fragment>
    <fragment
        android:id="@+id/upload_prescription"
        android:name="com.example.apnicare.upload_prescription"
        android:label="fragment_upload_prescription"
        tools:layout="@layout/fragment_upload_prescription" >
        <action
            android:id="@+id/action_upload_prescription_to_why_prescription"
            app:destination="@id/why_prescription" />
    </fragment>
    <fragment
        android:id="@+id/why_prescription"
        android:name="com.example.apnicare.why_prescription"
        android:label="fragment_why_prescription"
        tools:layout="@layout/fragment_why_prescription" >
        <action
            android:id="@+id/action_why_prescription_to_why_prescription_2"
            app:destination="@id/why_prescription_2" />
    </fragment>
    <fragment
        android:id="@+id/why_prescription_2"
        android:name="com.example.apnicare.why_prescription_2"
        android:label="fragment_why_prescription_2"
        tools:layout="@layout/fragment_why_prescription_2" />
    <fragment
        android:id="@+id/health_records_page"
        android:name="com.example.apnicare.health_records_page"
        android:label="fragment_health_records_page"
        tools:layout="@layout/fragment_health_records_page" />
    <fragment
        android:id="@+id/your_orders"
        android:name="com.example.apnicare.your_orders"
        android:label="fragment_your_orders"
        tools:layout="@layout/fragment_your_orders" />
    <fragment
        android:id="@+id/family_members"
        android:name="com.example.apnicare.family_members"
        android:label="fragment_family_members"
        tools:layout="@layout/fragment_family_members" />
    <fragment
        android:id="@+id/my_cart"
        android:name="com.example.apnicare.my_cart"
        android:label="fragment_my_cart"
        tools:layout="@layout/fragment_my_cart" />
    <fragment
        android:id="@+id/search_medicine"
        android:name="com.example.apnicare.search_medicine"
        android:label="fragment_search_medicine"
        tools:layout="@layout/fragment_search_medicine" />
</navigation>